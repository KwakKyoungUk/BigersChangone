	<queryString>
		<![CDATA[SELECT 
	ORDER_SEQ
	, @PRICE := IF(ABS(PRE_QTY+IN_QTY) = 0
		, (
			SELECT PRICE/(IF(DEFAULT_QTY = 0, 1, DEFAULT_QTY))
			FROM ITEM_PRICE
			WHERE PART_CODE = TT.PART_CODE
			AND ITEM_CODE = TT.ITEM_CODE
			AND KIND = 1
			AND ST_DATE = (
				SELECT MAX(ST_DATE)
				FROM ITEM_PRICE
				WHERE PART_CODE = TT.PART_CODE AND ITEM_CODE = TT.ITEM_CODE AND KIND = 1
				)
		)
		, ROUND((ABS(PRE_AMOUNT)+ABS(IN_AMOUNT))/(ABS(PRE_QTY)+ABS(IN_QTY)))
	) PRICE
	, ET_DATE
	, PART_CODE
	, CUSTOMER_NO
	, CUST_CODE
	, CUST_NAME
	, ITEM_CODE
	, ITEM_SORT
	, GROUP_SORT
	, ITEM_NAME
	, GROUP_NAME
	, UNIT
	, job_ym
	, PRE_QTY
	, PRE_QTY*@PRICE PRE_AMOUNT
	, IN_QTY
	, IN_QTY*@PRICE IN_AMOUNT
	, OUT_QTY
	, OUT_QTY*@PRICE OUT_AMOUNT
	, GAP_QTY
	, GAP_QTY*@PRICE COST_AMT
	, QTY
	, QTY*@PRICE AMOUNT 
  FROM (
			SELECT
				1 ORDER_SEQ
				, NOW() ET_DATE
				, c.PART_CODE
				, 1 CUSTOMER_NO
				, c.ITEM_CODE
				, c.ITEM_SORT
				, c.GROUP_SORT
				, c.ITEM_NAME
				, c.GROUP_NAME
				, c.UNIT
				, c.job_ym
				, c.CUST_CODE
				, c.CUST_NAME
				, SUM(c.qty)        AS PRE_QTY
				, SUM(c.amount)     AS PRE_AMOUNT
				, SUM(d.in_qty)     AS IN_QTY
				, SUM(d.in_amount)  AS IN_AMOUNT
				, SUM(d.out_qty)    AS OUT_QTY
				, SUM(d.out_amount) AS OUT_AMOUNT
				, SUM(d.adj_qty)    AS GAP_QTY
				, SUM(d.adj_amount) AS COST_AMT
				, SUM(c.qty+d.in_qty-d.out_qty-d.adj_qty) AS QTY
				, SUM(c.amount+d.in_amount-d.out_amount-d.adj_amount) AS AMOUNT
			FROM (
				SELECT
					  a.PART_CODE
					, IF(a.ITEM_GROUP_CODE = '' OR a.ITEM_GROUP_CODE IS NULL, a.ITEM_CODE, a.ITEM_GROUP_CODE) AS ITEM_CODE
					, a.SORT_NO ITEM_SORT
					, a.GROUP_CODE
					, ig.SORT_NO GROUP_SORT
					, a.ITEM_NAME
					, a.CUST_CODE
					, cust.CUST_NAME
					, ig.GROUP_NAME
					, a.UNIT
					#, a.ITEM_CODE                 AS ITEM_CODE
					, REPLACE('2018-01', '-', '') AS JOB_YM
					, (IFNULL(pre_qty, 0)         + IFNULL(buy_qty, 0)   +  IFNULL(production_stmt_qty, 0) ) - (IFNULL(sale_stmt_bd_qty, 0) + IFNULL(production_stmt_bd_cost_qty, 0) + IFNULL(stock_real_qty, 0) + IFNULL(stock_adj_qty, 0) ) AS qty
					, (IFNULL(pre_amount, 0)      + IFNULL(buy_amt, 0)   +  IFNULL(production_stmt_amt, 0) ) - (IFNULL(sale_stmt_bd_amt, 0) + IFNULL(production_stmt_bd_cost_amt, 0) + IFNULL(stock_real_amt, 0) + IFNULL(stock_adj_amt, 0) ) AS amount
				FROM item a
				JOIN item_group ig
				ON a.PART_CODE = ig.PART_CODE
				AND a.GROUP_CODE = ig.GROUP_CODE
				LEFT JOIN customer cust
				ON a.PART_CODE = cust.PART_CODE
				AND a.CUST_CODE = cust.CUST_CODE
				LEFT JOIN (
					SELECT part_code, item_code,
					      SUM(IF(a.gubun = '1', IFNULL(qty, 0),0)) AS buy_qty,             -- 매입
					      SUM(IF(a.gubun = '1', IFNULL(amt, 0),0)) AS buy_amt,
					      SUM(IF(a.gubun = '2', IFNULL(qty, 0),0)) AS production_stmt_qty, -- 생산입고
					      SUM(IF(a.gubun = '2', IFNULL(amt, 0),0)) AS production_stmt_amt,
					      SUM(IF(a.gubun = '3', IFNULL(qty, 0),0)) AS sale_stmt_bd_qty,    -- 판매
					      SUM(IF(a.gubun = '3', IFNULL(amt, 0),0)) AS sale_stmt_bd_amt,
					      SUM(IF(a.gubun = '4', IFNULL(qty, 0),0)) AS production_stmt_bd_cost_qty, -- 소모
					      SUM(IF(a.gubun = '4', IFNULL(amt, 0),0)) AS production_stmt_bd_cost_amt,
					      SUM(IF(a.gubun = '5', IFNULL(qty, 0),0)) AS stock_real_qty,     -- 실사
					      SUM(IF(a.gubun = '5', IFNULL(amt, 0),0)) AS stock_real_amt,
					      SUM(IF(a.gubun = '6', IFNULL(qty, 0),0)) AS stock_adj_qty,      -- 단수차이
					      SUM(IF(a.gubun = '6', IFNULL(amt, 0),0)) AS stock_adj_amt,
					      SUM(IF(a.gubun = '7', IFNULL(qty, 0),0)) AS PRE_QTY, -- 이월재고
					      SUM(IF(a.gubun = '7', IFNULL(amt, 0),0)) AS PRE_AMOUNT

					FROM (
						-- 매입
						SELECT '1'                       AS   gubun,
						       PART_CODE,
						       ITEM_CODE,
						       SUM(IFNULL(QTY,0))        AS qty,
						       SUM(IFNULL(a.T_AMOUNT,0)) AS amt
						  FROM buy_stmt_bd a
						 WHERE ET_DATE BETWEEN CONCAT(LEFT($P{FROM}, 7), '-01') AND DATE_ADD($P{FROM}, INTERVAL -1 DAY)
						 GROUP BY PART_CODE, ITEM_CODE

						UNION ALL

						-- 생산입고
						 SELECT '2'                      AS  gubun,
						        PART_CODE,
						        ITEM_CODE,
						        SUM(IFNULL(QTY,0))       AS  qty,
						        SUM(IFNULL(COST_AMT,0))  AS  amt
						   FROM production_stmt_bd
						  WHERE ET_DATE BETWEEN CONCAT(LEFT($P{FROM}, 7), '-01') AND DATE_ADD($P{FROM}, INTERVAL -1 DAY)
						  GROUP BY PART_CODE, ITEM_CODE

						UNION ALL


						-- 개별/빈소판매
						SELECT '3'                        AS gubun,
						        b.PART_CODE,
						        b.ITEM_CODE,
						        SUM(IFNULL(b.QTY,0))      AS  qty,
						        SUM(IFNULL(b.COST_AMT,0)) AS  amt
						  FROM sale_stmt     a
						  JOIN sale_stmt_bd  b
						    ON a.CUSTOMER_NO = b.CUSTOMER_NO
						   AND a.PART_CODE   = b.PART_CODE
						   AND a.BILL_NO     = b.BILL_NO
						 WHERE a.ET_DATE BETWEEN CONCAT(LEFT($P{FROM}, 7), '-01') AND DATE_ADD($P{FROM}, INTERVAL -1 DAY)
						 GROUP BY b.PART_CODE, b.ITEM_CODE

						UNION ALL


						-- 자재소모
						SELECT '4' AS gubun,
						       psbc.part_code,
						       psbc.MATE_CODE               AS  ITEM_CODE,
						       SUM(IFNULL(psbc.QTY,0))      AS  qty,
						       SUM(IFNULL(psbc.COST_AMT,0)) AS  amt
						  FROM production_stmt_bd_cost psbc
						  join production_stmt_bd psb
							  on psb.PART_CODE = psbc.PART_CODE
							 and psb.ET_DATE = psbc.ET_DATE
							 and psb.BILL_NO= psbc.BILL_NO
							 and psb.SEQ_NO = psbc.SEQ_NO
						  JOIN sale_stmt_bd ssb
						    ON psb.auto_customer_no = ssb.customer_no
						    AND psb.part_code = ssb.part_code
						    AND psb.auto_bill_no = ssb.bill_no
						    AND psb.auto_seq_no = ssb.seq_no
						    AND psb.item_code = ssb.item_code
						 WHERE psbc.ET_DATE BETWEEN CONCAT(LEFT($P{FROM}, 7), '-01') AND DATE_ADD($P{FROM}, INTERVAL -1 DAY)
						GROUP BY psbc.PART_CODE, psbc.MATE_CODE

						UNION ALL

						-- 자재소모 (식단)
						SELECT '4' AS gubun,
						       psbc.part_code,
						       psbc.MATE_CODE               AS  ITEM_CODE,
						       SUM(IFNULL(psbc.QTY,0))      AS  qty,
						       SUM(IFNULL(psbc.COST_AMT,0)) AS  amt
						  FROM production_stmt_bd_cost psbc
  						  join production_stmt_bd psb
							  on psb.PART_CODE = psbc.PART_CODE
							 and psb.ET_DATE = psbc.ET_DATE
							 and psb.BILL_NO= psbc.BILL_NO
							 and psb.SEQ_NO = psbc.SEQ_NO
						 where psb.item_code = '90-9998'  -- 자동샌산품목
						   and psbc.ET_DATE BETWEEN CONCAT(LEFT($P{FROM}, 7), '-01') AND DATE_ADD($P{FROM}, INTERVAL -1 DAY)
						GROUP BY psbc.PART_CODE, psbc.MATE_CODE

						UNION ALL

						-- 실사

						SELECT 	'5' AS GUBUN,
						       PART_CODE,
						       ITEM_CODE,
						       SUM(IFNULL(GAP_QTY,0))   AS qty ,
						       SUM(IFNULL(COST_AMT,0))  AS amt
						  FROM stock_real
						 WHERE ET_DATE BETWEEN CONCAT(LEFT($P{FROM}, 7), '-01') AND DATE_ADD($P{FROM}, INTERVAL -1 DAY)
						 GROUP BY PART_CODE, ITEM_CODE

						UNION ALL

						-- 단수차이
						SELECT '6'                    AS GUBUN,
						       PART_CODE,
						       ITEM_CODE,
						       0                      AS qty,
						       SUM(IFNULL(amount,0))  AS amt
						  FROM stock_adj
						 WHERE ET_DATE BETWEEN CONCAT(LEFT($P{FROM}, 7), '-01') AND DATE_ADD($P{FROM}, INTERVAL -1 DAY)
						 GROUP BY part_code, item_code

						 UNION ALL

						 -- 이월재고
						SELECT '7' AS GUBUN, PART_CODE, ITEM_CODE, IFNULL(QTY, 0) qty, IFNULL(AMOUNT, 0) AMT
						FROM STOCK
						WHERE JOB_YM = REPLACE(LEFT(DATE_ADD(CONCAT(LEFT($P{FROM}, 7), '-01'), INTERVAL -1 MONTH), 7), '-', '')

					 ) a
					  GROUP BY part_code, item_code
				) b
				ON a.PART_CODE  = b.PART_CODE
				AND a.ITEM_CODE = b.ITEM_CODE
				WHERE a.ITEM_KIND_CODE IN ('00', '60')  -- 매입+판매, 매입+소모 품목만 재고 대상
				AND a.USE_FLG = 'Y'
			) c
			LEFT JOIN (
				SELECT
					  a.PART_CODE
					, IF(a.ITEM_GROUP_CODE = '' OR a.ITEM_GROUP_CODE IS NULL, a.ITEM_CODE, a.ITEM_GROUP_CODE) AS ITEM_CODE
					#, a.ITEM_CODE                 AS ITEM_CODE
					, REPLACE('2018-01', '-', '') AS JOB_YM
					, IFNULL(buy_qty, 0)          +  IFNULL(production_stmt_qty, 0)          AS IN_QTY
					, IFNULL(buy_amt, 0)          +  IFNULL(production_stmt_amt, 0)          AS IN_AMOUNT
					, IFNULL(sale_stmt_bd_qty, 0) +  IFNULL(production_stmt_bd_cost_qty, 0)  AS OUT_QTY
					, IFNULL(sale_stmt_bd_amt, 0) +  IFNULL(production_stmt_bd_cost_amt, 0)  AS OUT_AMOUNT
					, IFNULL(stock_real_qty, 0)                                   AS ADJ_QTY
					, IFNULL(stock_real_amt, 0)   +  IFNULL(stock_adj_amt, 0)                AS ADJ_AMOUNT
					, (IFNULL(buy_qty, 0)   +  IFNULL(production_stmt_qty, 0) ) - (IFNULL(sale_stmt_bd_qty, 0) + IFNULL(production_stmt_bd_cost_qty, 0) + IFNULL(stock_real_qty, 0) + IFNULL(stock_adj_qty, 0) ) AS qty
					, (IFNULL(buy_amt, 0)   +  IFNULL(production_stmt_amt, 0) ) - (IFNULL(sale_stmt_bd_amt, 0) + IFNULL(production_stmt_bd_cost_amt, 0) + IFNULL(stock_real_amt, 0) + IFNULL(stock_adj_amt, 0) ) AS amount
				FROM item a
				LEFT JOIN (
					SELECT part_code, item_code,
					      SUM(IF(a.gubun = '1', IFNULL(qty, 0),0)) AS buy_qty,             -- 매입
					      SUM(IF(a.gubun = '1', IFNULL(amt, 0),0)) AS buy_amt,
					      SUM(IF(a.gubun = '2', IFNULL(qty, 0),0)) AS production_stmt_qty, -- 생산입고
					      SUM(IF(a.gubun = '2', IFNULL(amt, 0),0)) AS production_stmt_amt,
					      SUM(IF(a.gubun = '3', IFNULL(qty, 0),0)) AS sale_stmt_bd_qty,    -- 판매
					      SUM(IF(a.gubun = '3', IFNULL(amt, 0),0)) AS sale_stmt_bd_amt,
					      SUM(IF(a.gubun = '4', IFNULL(qty, 0),0)) AS production_stmt_bd_cost_qty, -- 소모
					      SUM(IF(a.gubun = '4', IFNULL(amt, 0),0)) AS production_stmt_bd_cost_amt,
					      SUM(IF(a.gubun = '5', IFNULL(qty, 0),0)) AS stock_real_qty,     -- 실사
					      SUM(IF(a.gubun = '5', IFNULL(amt, 0),0)) AS stock_real_amt,
					      SUM(IF(a.gubun = '6', IFNULL(qty, 0),0)) AS stock_adj_qty,      -- 단수차이
					      SUM(IF(a.gubun = '6', IFNULL(amt, 0),0)) AS stock_adj_amt

					FROM (
						-- 매입
						SELECT '1'                       AS   gubun,
						       PART_CODE,
						       ITEM_CODE,
						       SUM(IFNULL(QTY,0))        AS qty,
						       SUM(IFNULL(a.T_AMOUNT,0)) AS amt
						  FROM buy_stmt_bd a
						 WHERE ET_DATE BETWEEN $P{FROM} AND $P{TO}
						 GROUP BY PART_CODE, ITEM_CODE

						UNION ALL

						-- 생산입고
						 SELECT '2'                      AS  gubun,
						        PART_CODE,
						        ITEM_CODE,
						        SUM(IFNULL(QTY,0))       AS  qty,
						        SUM(IFNULL(COST_AMT,0))  AS  amt
						   FROM production_stmt_bd
						  WHERE ET_DATE BETWEEN $P{FROM} AND $P{TO}
						  GROUP BY PART_CODE, ITEM_CODE

						UNION ALL


						-- 개별/빈소판매
						SELECT '3'                        AS gubun,
						        b.PART_CODE,
						        b.ITEM_CODE,
						        SUM(IFNULL(b.QTY,0))      AS  qty,
						        SUM(IFNULL(b.COST_AMT,0)) AS  amt
						  FROM sale_stmt     a
						  JOIN sale_stmt_bd  b
						    ON a.CUSTOMER_NO = b.CUSTOMER_NO
						   AND a.PART_CODE   = b.PART_CODE
						   AND a.BILL_NO     = b.BILL_NO
						 WHERE a.ET_DATE BETWEEN $P{FROM} AND $P{TO}
						 GROUP BY b.PART_CODE, b.ITEM_CODE

						UNION ALL


						-- 자재소모
						SELECT '4' AS gubun,
						       psbc.part_code,
						       psbc.MATE_CODE               AS  ITEM_CODE,
						       SUM(IFNULL(psbc.QTY,0))      AS  qty,
						       SUM(IFNULL(psbc.COST_AMT,0)) AS  amt
						  FROM production_stmt_bd_cost psbc
						  join production_stmt_bd psb
							  on psb.PART_CODE = psbc.PART_CODE
							 and psb.ET_DATE = psbc.ET_DATE
							 and psb.BILL_NO= psbc.BILL_NO
							 and psb.SEQ_NO = psbc.SEQ_NO
							JOIN sale_stmt_bd ssb
						    ON psb.auto_customer_no = ssb.customer_no
						    AND psb.part_code = ssb.part_code
						    AND psb.auto_bill_no = ssb.bill_no
						    AND psb.auto_seq_no = ssb.seq_no
						    AND psb.item_code = ssb.item_code
						 WHERE psbc.ET_DATE BETWEEN $P{FROM} AND $P{TO}
						GROUP BY psbc.PART_CODE, psbc.MATE_CODE

						UNION ALL

						-- 자재소모 (식단)
						SELECT '4' AS gubun,
						       psbc.part_code,
						       psbc.MATE_CODE               AS  ITEM_CODE,
						       SUM(IFNULL(psbc.QTY,0))      AS  qty,
						       SUM(IFNULL(psbc.COST_AMT,0)) AS  amt
						  FROM production_stmt_bd_cost psbc
  						  join production_stmt_bd psb
							  on psb.PART_CODE = psbc.PART_CODE
							 and psb.ET_DATE = psbc.ET_DATE
							 and psb.BILL_NO= psbc.BILL_NO
							 and psb.SEQ_NO = psbc.SEQ_NO
						 where psb.item_code = '90-9998'  -- 자동샌산품목
						   and psbc.ET_DATE BETWEEN $P{FROM} AND $P{TO}
						GROUP BY psbc.PART_CODE, psbc.MATE_CODE

						UNION ALL

						-- 실사

						SELECT 	'5' AS GUBUN,
						       PART_CODE,
						       ITEM_CODE,
						       SUM(IFNULL(GAP_QTY,0))   AS qty ,
						       SUM(IFNULL(COST_AMT,0))  AS amt
						  FROM stock_real
						 WHERE ET_DATE BETWEEN $P{FROM} AND $P{TO}
						 GROUP BY PART_CODE, ITEM_CODE

						UNION ALL

						-- 단수차이
						SELECT '6'                    AS GUBUN,
						       PART_CODE,
						       ITEM_CODE,
						       0                      AS qty,
						       SUM(IFNULL(amount,0))  AS amt
						  FROM stock_adj
						 WHERE ET_DATE BETWEEN $P{FROM} AND $P{TO}
						 GROUP BY part_code, item_code

					 ) a
					  GROUP BY a.part_code, a.item_code
				) b
				ON a.PART_CODE  = b.PART_CODE
				AND a.ITEM_CODE = b.ITEM_CODE
				WHERE a.ITEM_KIND_CODE IN ('00', '60')  -- 매입+판매, 매입+소모 품목만 재고 대상
				AND a.USE_FLG = 'Y'
			) d
			ON c.PART_CODE  = d.PART_CODE
			AND c.ITEM_CODE = d.ITEM_CODE

		WHERE c.PART_CODE = $P{partCode}
		AND c.group_code LIKE CONCAT($P{groupCode}, '%')
		GROUP BY c.part_code, c.item_code
# 20210421		HAVING !(SUM(c.qty) = 0
#				AND SUM(c.amount) = 0
# 20210421				AND SUM(d.in_qty) = 0
#				AND SUM(d.in_amount) = 0
# 20210421				AND SUM(d.out_qty) = 0
#				AND SUM(d.out_amount) = 0
# 20210421				AND SUM(d.adj_qty) = 0
#				AND SUM(d.adj_amount) = 0
# 20210421				AND SUM(c.qty+d.in_qty-d.out_qty-d.adj_qty) = 0
#				AND SUM(c.amount+d.in_amount-d.out_amount-d.adj_amount) = 0
# 20210421		)
		ORDER BY c.CUST_CODE, c.GROUP_SORT, c.GROUP_CODE, c.ITEM_SORT, c.ITEM_CODE
) TT]]>
	</queryString>
	<field name="ORDER_SEQ" class="java.lang.Long"/>
	<field name="ET_DATE" class="java.sql.Date"/>
	<field name="PART_CODE" class="java.lang.String"/>
	<field name="CUSTOMER_NO" class="java.lang.String"/>
	<field name="ITEM_CODE" class="java.lang.String"/>
	<field name="ITEM_NAME" class="java.lang.String"/>
	<field name="GROUP_NAME" class="java.lang.String"/>
	<field name="UNIT" class="java.lang.String"/>
	<field name="PRE_QTY" class="java.math.BigDecimal"/>
	<field name="PRE_AMOUNT" class="java.math.BigDecimal"/>
	<field name="IN_QTY" class="java.math.BigDecimal"/>
	<field name="IN_AMOUNT" class="java.math.BigDecimal"/>
	<field name="OUT_QTY" class="java.math.BigDecimal"/>
